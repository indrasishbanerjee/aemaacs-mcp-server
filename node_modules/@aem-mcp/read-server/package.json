{
  "name": "@aem-mcp/read-server",
  "version": "1.0.0",
  "description": "AEM as a Cloud Service Read Operations MCP Server",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "aem-read-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index.js --stdio",
    "dev": "tsx src/index.ts",
    "dev:stdio": "tsx src/index.ts --stdio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "tsx": "^4.6.2"
  },
  "keywords": [
    "aem",
    "adobe",
    "mcp",
    "read-operations",
    "content-management"
  ],
  "author": "AEM MCP Team",
  "license": "MIT"
}